<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<!-- [DB연결 사용법] 8. 만든 DTO와 Mapper정의하기 -->
	<typeAliases>
		<!-- <typeAlias type="com.spring.travel.dto.UserDTO" alias="UserDTO"></typeAlias> -->
		<typeAlias type="kr.co.upi.DTO.IndicatorsDTO" alias="IndicatorsDTO"></typeAlias>
		<typeAlias type="kr.co.upi.DTO.ReportDTO" alias="ReportDTO"></typeAlias>
		<typeAlias type="kr.co.upi.DTO.GradeDTO" alias="GradeDTO"></typeAlias>
		<typeAlias type="kr.co.upi.DTO.RecordDTO" alias="RecordDTO"></typeAlias>
		<typeAlias type="kr.co.upi.DTO.ReportDTO" alias="ReportDTO"></typeAlias>
	</typeAliases>

	<mappers>
		<!-- <mapper resource="mappers/UserMapper.xml" /> -->
		<mapper resource="mappers/TestMapper.xml" />
		<mapper resource="mappers/ResultMapper.xml" />
		<mapper resource="mappers/IndicatorsMapper.xml" />
		<mapper resource="mappers/ReportMapper.xml" />
	</mappers>
</configuration>
